{
  "name": "address-to-envelope",
  "version": "0.0.0",
  "scripts": {
    "prGatekeeper": "ng build && prettier --check . && compodoc -c compodocrc.json --coverageMinimumPerFile 100 -t && stylelint **/*.scss && ng lint addressToEnvelope",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "commitlint": "commitlint",
    "lint:check": "ng lint addressToEnvelope",
    "prettier:check": "prettier --check .",
    "docs:check": "compodoc -c compodocrc.json --coverageMinimumPerFile 100 -t",
    "stylelint:check": "stylelint **/*.scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.4",
    "@angular/cli": "~20.3.4",
    "@angular/compiler-cli": "^20.3.3",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@compodoc/compodoc": "^1.1.30",
    "@eslint/css": "^0.11.1",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.3.0",
    "@types/jasmine": "~4.3.0",
    "angular-eslint": "20.3.0",
    "electron": "^38.2.1",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.40.0"
  }
}
